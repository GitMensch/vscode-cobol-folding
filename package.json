{
	"name": "cobol-folding",
	"displayName": "Cobol Folding",
	"description": "Add foldings to Cobol files",
	"version": "0.1.0",
	"publisher": "zokugun",
	"homepage": "https://github.com/zokugun/vscode-cobol-folding",
	"repository": {
		"type": "git",
		"url": "https://github.com/zokugun/vscode-cobol-folding.git"
	},
	"bugs": {
		"url": "https://github.com/zokugun/vscode-cobol-folding/issues"
	},
	"author": {
		"name": "Baptiste Augrain",
		"email": "daiyam@zokugun.org"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.23.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Cobol Folding configuration",
			"properties": {
				"cobolFolding.enabled": {
					"type": "boolean",
					"default": true,
					"description": ""
				}
			}
		}
	},
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"dependencies": {
		"@zokugun/vscode.explicit-folding-api": "0.1.0"
	},
	"devDependencies": {
		"@types/node": "^10.17.17",
		"@types/vscode": "^1.23.0",
		"typescript": "^4.2.4"
	},
	"extensionDependencies": [
		"zokugun.explicit-folding"
	],
	"keywords": [
		"folding",
		"cobol"
	],
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#373b41",
		"theme": "dark"
	}
}
